				},
				"url": {
					"raw": "{{baseUrl}}/api/v1/experts/{{expertId}}:preflight",
					"host": [
						"{{baseUrl}}"
					],
					"path": [
						"api",
						"v1",
						"experts",
						"{{expertId}}:preflight"
					]
				}
			},
			"response": []
